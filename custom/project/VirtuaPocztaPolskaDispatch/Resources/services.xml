<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service class="VirtuaPocztaPolskaDispatch\Services\DispatchCreator" id="virtua_poczta_polska_dispatch.services.dispatch_creator">
            <argument id="models" type="service"/>
        </service>

        <service class="VirtuaPocztaPolskaDispatch\Subscriber\Frontend" id="virtua_poczta_polska_dispatch.subscriber.frontend">
            <tag name="shopware.event_subscriber"/>
        </service>


    </services>
